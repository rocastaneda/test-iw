  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch via NPM",
      "runtimeExecutable": "npm",
      "runtimeArgs": ["run-start", "debug"],
      "port": 6001
    },
    {
      "type": "chrome",
      "request": "launch",
      "name": "Launch Chrome against localhost",
      "url": "http://localhost:6001",
      "webRoot": "${workspaceFolder}",
      "runtimeExecutable": "/usr/share/man/man1/google-chrome.1.gz"
    }
  ]
}
